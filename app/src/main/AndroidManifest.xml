<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.udacitymusicexample.udacitymusic">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".HomeActivity"
            android:label="@string/category_home" />
        <activity
            android:name=".SearchActivity"
            android:label="@string/category_search" />
        <activity
            android:name=".LibraryActivity"
            android:label="@string/category_library" />
        <activity
            android:name=".AlbunsActivity"
            android:label="@string/category_albuns" />
        <activity
            android:name=".AllSongs"
            android:label="@string/category_all_songs" />
        <activity
            android:name=".ArtistActivity"
            android:label="@string/category_artists" />
    </application>

</manifest>